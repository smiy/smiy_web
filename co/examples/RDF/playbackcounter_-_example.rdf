    <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2010-07-15T11:31:52+01:00</dc:date>
    <dc:description xml:lang="en">This document includes an example of the counter concept of the Counter Ontology.</dc:description>
    <dc:title xml:lang="en">Play Back Counter Example</dc:title>
