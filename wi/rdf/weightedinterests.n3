wi:topic
      rdf:type owl:ObjectProperty , rdf:Property ;
      rdfs:comment "A topic of the weighted interest"@en ;
      rdfs:domain wi:WeightedInterest ;
      rdfs:isDefinedBy wi: ;
      rdfs:label "has topic"@en ;
