      status:term_status "testing"@en .

wi:preference
      rdf:type owl:ObjectProperty , rdf:Property ;
      rdfs:comment "A link between an agent and a weighted interest"@en ;
      rdfs:domain foaf:Agent ;
      rdfs:isDefinedBy wi: ;
      rdfs:label "preference"@en ;
