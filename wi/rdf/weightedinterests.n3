wi:weight
      rdf:type rdf:Property , owl:ObjectProperty ;
      rdfs:comment "The weight on the topic, which should normally reflect the actual interest in a topic."@en ;
      rdfs:domain wi:WeightedInterest ;
      rdfs:isDefinedBy wi: ;
