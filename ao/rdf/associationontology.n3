@prefix ao:      <http://purl.org/ontology/ao/core#> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sim:     <http://purl.org/ontology/similarity/> .
@prefix vs:      <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

ao:   rdf:type owl:Ontology ;
      dc:creator <http://foaf.me/zazi#me> ;
      dc:date "2010-07-20T14:44:52+01:00"^^xsd:dateTime ;
      dc:description """The Association Ontology specification provides basic properties 
for describing a associations to something, e.g. an occasion, a genre ar a mood 
on/ for the Semantic Web. This document contains a RDF description of the Association Ontology."""@en ;
      dc:title "The Association Ontology"@en ;
      owl:imports dcterms: , dc: , <http://www.w3.org/2003/06/sw-vocab-status/ns> , sim: ;
      owl:versionInfo 0.1 .

ao:association
      rdf:type owl:ObjectProperty ;
      rdfs:comment "An association property to related associations to something, e.g. a association that includes an ao:mood, an ao:genre and an ao:occasion."^^xsd:string ;
      rdfs:domain owl:Thing ;
      rdfs:isDefinedBy ao: ;
      rdfs:label "association"^^xsd:string ;
      rdfs:range sim:Association ;
      vs:term_status "testing"^^xsd:string .

ao:genre
      rdf:type rdf:Property ;
      rdfs:comment "An genre of something."@en ;
      rdfs:domain owl:Thing ;
      rdfs:isDefinedBy ao: ;
      rdfs:label "has genre"@en ;
      rdfs:subPropertyOf dcterms:subject ;
      owl:inverseOf
              [ rdf:type rdf:Property ;
                rdfs:label "is genre of"@en
              ] ;
      vs:term_status "unstable"@en .

ao:likeminded
      rdf:type owl:ObjectProperty ;
      rdfs:comment "Associates likeminded people to an association, which they like/ agree with."^^xsd:string ;
      rdfs:domain sim:Association ;
      rdfs:isDefinedBy ao: ;
      rdfs:label "has likeminded"^^xsd:string ;
      rdfs:range <http://xmlns.com/foaf/0.1/Agent> ;
      vs:term_status "testing"^^xsd:string .

ao:mood
      rdf:type rdf:Property ;
      rdfs:comment "A mood that should be created by something."@en ;
      rdfs:domain owl:Thing ;
      rdfs:isDefinedBy ao: ;
      rdfs:label "has mood"@en ;
      rdfs:subPropertyOf dcterms:subject ;
      owl:inverseOf
              [ rdf:type rdf:Property ;
                rdfs:label "is mood of"@en
              ] ;
      vs:term_status "unstable"@en .

ao:occasion
      rdf:type rdf:Property ;
      rdfs:comment "An occasion for which something was created."@en ;
      rdfs:domain owl:Thing ;
      rdfs:isDefinedBy ao: ;
      rdfs:label "has occassion"@en ;
      rdfs:subPropertyOf dcterms:subject ;
      owl:inverseOf
              [ rdf:type rdf:Property ;
                rdfs:label "is occassion of"@en
              ] ;
      vs:term_status "unstable"@en .
